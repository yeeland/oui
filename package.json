    "babel-jest": "^11.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.9.8",
    "del": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^5.0.1",
    "fs": "0.0.2",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-base64": "^0.1.3",
    "gulp-concat": "^2.6.0",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.0.2",
    "gulp-shell": "^0.5.1",
    "gulp-symlink": "^2.1.0",
    "gulp-tap": "^0.1.3",
    "gulp-uglifyjs": "^0.6.2",
    "gulp-util": "^3.0.0",
    "jest-cli": "^11.0.2",
    "mkpath": "^1.0.0",
    "onchange": "^2.4.0",
    "oui-icons": "^0.1.2",
    "parallelshell": "^2.0.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "require-dir": "^0.3.0",
    "webpack": "^1.12.14",
    "yargs": "^4.1.0"
  },
  "scripts": {
    "test": "npm run scss-lint && npm run eslint && gulp sass && npm run jest",
    "start": "bundle install && bundle update && npm install",
    "jest": "jest --config .jest.config.json",
    "eslint": "eslint --ext .jsx --ext .js . --ignore-path .gitignore --max-warnings 0",
    "scss-lint": "bundle exec scss-lint src/",
    "preversion": "git checkout master && git pull --rebase=false && npm install",
    "postversion": "npm run postversion:publish && npm run postversion:update-devel",
    "postversion:publish": "git push && git push --tags",
    "postversion:update-devel": "git checkout devel && git pull && git merge master && git push origin devel && git checkout master",
    "react": "gulp sass && cp ./src/index.html ./dist/index.html && webpack && browser-sync reload",
    "react:serve": "npm run react && parallelshell 'npm run react:onchange' 'npm run react:browser-sync'",
    "react:onchange": "onchange 'src/**/*.(jsx|js)' -- npm run react",
    "react:browser-sync": "browser-sync start --server dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/optimizely/oui.git"
  },
  "engines": {
    "node": ">=0.12.0"
  },
